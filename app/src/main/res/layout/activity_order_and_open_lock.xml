<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_contanier"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">

    <include layout="@layout/layout_header_view" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="30dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="13sp"
            android:text="预约与开关锁"/>
    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/rl_realname_no_pass"
        style="@style/RlStyle">

        <TextView style="@style/TvStyle"
            android:text="预约能为我保留多久"/>

        <ImageView
            android:id="@+id/iv_next"
            style="@style/IvNextStyle" />


    </RelativeLayout>
    <View style="@style/LineStyle" />

    <RelativeLayout
        android:id="@+id/rl_realname_isexist"
        style="@style/RlStyle">

        <TextView
            style="@style/TvStyle"
            android:text="预约后怎么找到单车" />

        <ImageView
            android:id="@+id/iv_nick_name"
            style="@style/IvNextStyle" />

    </RelativeLayout>
    <View style="@style/LineStyle" />

    <RelativeLayout
        android:id="@+id/rl_about_invitation_code"
        style="@style/RlStyle">

        <TextView
            style="@style/TvStyle"
            android:text="如何关锁" />
        <ImageView
            style="@style/IvNextStyle" />

    </RelativeLayout>

</LinearLayout>