<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_contanier"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">

    <include layout="@layout/layout_header_view" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:gravity="center_vertical"
        android:layout_height="30dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:textSize="13sp"
            android:text="车费与押金"/>
    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/rl_realname_no_pass"
        style="@style/RlStyle">

        <TextView style="@style/TvStyle"
            android:text="为什么要交押金，押金与车费有什么不同？"/>

        <ImageView
            android:id="@+id/iv_next"
            style="@style/IvNextStyle" />


    </RelativeLayout>
    <View style="@style/LineStyle" />

    <RelativeLayout
        android:id="@+id/rl_realname_isexist"
        style="@style/RlStyle">

        <TextView
            style="@style/TvStyle"
            android:text="押金退款已超7个工作日还没到账。" />

        <ImageView
            android:id="@+id/iv_nick_name"
            style="@style/IvNextStyle" />

    </RelativeLayout>
    <View style="@style/LineStyle" />

    <RelativeLayout
        android:id="@+id/rl_about_invitation_code"
        style="@style/RlStyle">

        <TextView
            style="@style/TvStyle"
            android:text="假如我骑行费用超过余额，导致余额为负怎么办？" />
        <ImageView
            style="@style/IvNextStyle" />

    </RelativeLayout>
    <View style="@style/LineStyle" />

    <RelativeLayout
        android:id="@+id/rl_modifi_phone"
        style="@style/RlStyle">

        <TextView
            style="@style/TvStyle"
            android:text="使用小强单车怎么收费？" />

        <ImageView
            style="@style/IvNextStyle" />

    </RelativeLayout>

</LinearLayout>