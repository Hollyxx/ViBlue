<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_seletor</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:maxHeight">10.0dip</item>
    </style>

    <style name="TranslucentTheme" >
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="TitleTv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="TitleBackStyle">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="HomeBootomStyle">
        <item name="android:layout_width">20dp</item>="true"
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@drawable/rb_bottom_selector</item>
    </style>


    <style name="TvFneLeiStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/home_rb_textcolor</item>
    </style>

    <style name="RlFneLeiStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>


    <style name="IvDotStyle">
        <item name="android:layout_width">6dp</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>

    <style name="LlStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginLeft">9dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="LlStyle1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">#dcdcdc</item>
    </style>


    <style name="ivDrawerStyle">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_width">20dp</item>
    </style>

    <style name="textviewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">#262626</item>
    </style>

    <style name="llDrawerStyle">
        <item name="android:clickable">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/white_btn_seletor</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="ImageButtonStyle">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="tittleStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@color/colorPrimaryDark</item>
    </style>

    <style name="RlStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/white_btn_seletor</item>
    </style>

    <style name="TvStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:text">头像</item>
        <item name="android:textColor">#252525</item>
    </style>
    <style name="TvStyle1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:text">头像</item>
        <item name="android:textColor">#252525</item>
    </style>

    <style name="llrunStyle">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_below">@+id/rl_title</item>
        <item name="android:background">@color/theme_orange</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="LineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.1dp</item>
        <item name="android:background">#dcdcdc</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>
    <style name="IvNextStyle">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:padding">18dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@mipmap/user_list_next</item>
    </style>

    <style name="RechangeStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/desposit_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="RechangeStyle1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/desposit_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="RechangeTvStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="IvHeaderStyle">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:src">@mipmap/home_user_head</item>
    </style>
    <style name="IvSeachStyle">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:src">@drawable/home_btn_seach_seletor</item>
    </style>

    <style name="DrawerStyle">
        <item name="android:layout_width">240dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">start</item>
    </style>

    <style name="IvLogoStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="line1Style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#dcdcdc</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="ZxingStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@drawable/home_btn_zxing_seletor</item>
    </style>

    <style name="RlbStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_below">@id/rl_title</item>
        <item name="android:layout_marginTop">40dp</item>
    </style>


    <style name="EtStyle">
        <item name="android:layout_toRightOf">@id/iv_home_seach</item>
        <item name="android:layout_toLeftOf">@id/tv_cancel</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:hint">去哪找我</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLength">15</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">#f5f5f5</item>
        <item name="android:textColor">@color/background</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!-- 自定义loading dialog -->
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground"> @android:color/transparent </item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
    <!--  <item name="android:windowBackground">@mipmap/loading_bg</item>-->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


</resources>
